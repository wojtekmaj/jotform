{
  "name": "@wojtekmaj/jotform",
  "version": "2.1.0",
  "description": "Jotform API Node.js Client",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "node -e \"fs.rmSync('./dist', { recursive: true, force: true })\"",
    "format": "biome format",
    "lint": "biome lint",
    "postinstall": "husky",
    "prepack": "yarn clean && yarn build",
    "test": "yarn lint && yarn tsc && yarn format && yarn unit",
    "tsc": "tsc",
    "unit": "vitest"
  },
  "keywords": [
    "jotform",
    "api",
    "client",
    "nodejs"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "object-to-formdata": "^4.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@types/node": "*",
    "@wojtekmaj/async-array-utils": "^2.0.0",
    "husky": "^9.0.0",
    "p-throttle": "^7.0.0",
    "typescript": "^5.5.2",
    "vitest": "^3.0.5",
    "zod": "^3.22.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wojtekmaj/jotform.git"
  },
  "funding": "https://github.com/wojtekmaj/jotform?sponsor=1",
  "packageManager": "yarn@4.9.1"
}
